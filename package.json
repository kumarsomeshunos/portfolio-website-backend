{
   "name": "portfolio-website-backend",
   "version": "1.0.0",
   "description": "Backend for portfolio website of Somesh Kumar",
   "main": "src/index.js",
   "scripts": {
      "build": "./node_modules/.bin/babel src --out-dir lib",
      "dev": "nodemon src/index.js",
      "run_babel": "npm run build && node lib/index.js",
      "start": "node src/index.js",
      "test": "echo \"Error: no test specified\" && exit 1",
      "prepare": "husky install"
   },
   "lint-staged": {
      "**/*": "prettier --write --ignore-unknown"
   },
   "husky": {
      "hooks": {
         "pre-commit": "prettier --write . && git add -A ."
      }
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/kumarsomeshunos/portfolio-website-backend.git"
   },
   "author": "Somesh Kumar",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/kumarsomeshunos/portfolio-website-backend/issues"
   },
   "homepage": "https://github.com/kumarsomeshunos/portfolio-website-backend#readme",
   "dependencies": {
      "axios": "^1.6.2",
      "bcrypt": "^5.1.1",
      "cookie-parser": "^1.4.6",
      "core-js": "^3.32.2",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.2",
      "mongoose": "^7.5.0",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "postmark": "^4.0.2"
   },
   "devDependencies": {
      "@babel/cli": "^7.22.10",
      "@babel/core": "^7.22.11",
      "@babel/preset-env": "^7.22.14",
      "husky": "^8.0.3",
      "lint-staged": "^14.0.1",
      "nodemon": "^3.0.1",
      "prettier": "3.0.3"
   }
}
